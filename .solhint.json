{
    "extends": "solhint:recommended",
    "plugins": [],
    "rules": {
        "max-line-length": ["error",99],
        "avoid-suicide": "error",
        "avoid-sha3": "warn",
        "not-rely-on-time": "off",
        "compiler-version": ["error", ">=0.7.0 <0.9.0"],
        "reason-string": ["warn",{"maxLength":60}],
        "func-visibility": ["warn",{"ignoreConstructors":true}]
    }
}
